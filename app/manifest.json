{
  "_version": "1.1.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "sap.ui.app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "0.1.0"
    }
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "SAPUI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "rootView": "sap.ui.app.view.App",
    "dependencies": {
      "minUI5Version": "1.38.8",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ui.app.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
      {
        "uri": "assets/stylesheets/app.css"
      }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.ui.app.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [{
        "pattern": "",
        "name": "main",
        "target": "main"
      }],
      "targets": {
        "main": {
          "viewName": "Main"
        }
      }
    }
  }
}
